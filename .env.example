# - MySQL/MariaDB: USER:PASS@tcp(HOST:PORT)/DB?parseTime=true
# - Postgres: postgres://USER:PASS@HOST:PORT/DB
DB_CONN="file:db.sqlite3"
DB_TYPE="sqlite3"

